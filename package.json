{
  "name": "portt",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev-product": "npm run --workspace apps/web/productWeb/ dev",
    "dev-marketing": "npm run --workspace apps/web/marketingWeb/ dev"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.1",
  "dependencies": {
    "@portt/ui": "workspace:^0.0.0"
  }
}
